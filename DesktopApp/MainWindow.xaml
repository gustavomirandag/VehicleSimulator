        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="lbSpeed" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="203,70,0,0" FontSize="36"/>
        <Button x:Name="btnSpeedUp" Content="SpeedUp" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="138,146,0,0" Height="44" Click="btnSpeedUp_Click"/>
        <Button x:Name="btnSpeedDown" Content="SpeedDown" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="270,146,0,0" Height="44" Click="btnSpeedDown_Click"/>

    </Grid>
</Window>
